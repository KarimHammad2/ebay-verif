{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERIFICATION_TOKEN": "ebay_verification_token_2026_n8n_prod",
    "ENDPOINT_URL": "https://ebay-veri.vercel.app/"
  }
}
